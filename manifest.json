{
    "manifest_version": 3,
    "name": "Login Page Detector",
    "version": "1.0",
    "permissions": ["tabs", "scripting", "activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/Logo.png",
        "48": "assets/Logo.png",
        "128": "assets/Logo.png"
      }
    },
    "icons": {
      "16": "assets/Logo.png",
      "48": "assets/Logo.png",
      "128": "assets/Logo.png"
    },
    "host_permissions": ["<all_urls>"]
  }